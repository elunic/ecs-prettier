{
  "name": "@elunic/ecs-prettier",
  "version": "0.1.2",
  "description": "The elunic coding styles for Prettier",
  "files": [
    "prettier/",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "check": "prettier --check './**/*.js'",
    "fix": "prettier --write './**/*.js'",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elunic/ecs-prettier.git"
  },
  "keywords": [
    "elunic",
    "prettier",
    "styles",
    "ecs"
  ],
  "author": {
    "name": "William Hefter",
    "email": "wh@elunic.com"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0"
  },
  "dependencies": {
    "prettier": "^1.19.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
